<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/plantilla_sin_letras"
    tools:context="com.a0ohm.ledpool.contoler.subUsuarioGestor">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:gravity="center_horizontal"
            android:text="SUBUSUARIO"
            />
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">
    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    <TableRow
        android:background="@android:color/white"
        android:layout_height="match_parent"
            android:layout_width="wrap_content">
            <TextView
                android:text="Usuarios"
                android:layout_width="55dp"
                android:layout_height="wrap_content" />
        <TextView  android:text="Telefono"
            android:layout_width="100dp"
            android:layout_height="wrap_content"/>
        <TextView  android:text="Nombre"
            android:layout_width="100dp"
            android:layout_height="wrap_content"/>
        <TextView  android:text="Agregar"
            android:layout_width="50dp"
            android:layout_height="wrap_content"/>
        <TextView  android:text="Eliminar"
            android:layout_width="50dp"
            android:layout_height="wrap_content"/>
        <TextView  android:text="Codigo"
            android:layout_width="100dp"
            android:layout_height="wrap_content"/>
        </TableRow>
        <TableRow
            android:background="@android:color/white"
            android:layout_height="match_parent"
            android:layout_width="match_parent">
            <TextView  android:text="        1"
                android:layout_width="4dp"
                android:layout_height="wrap_content"/>
            <EditText
                android:id="@+id/Phone1"
                android:hint=""
                android:inputType="phone"
                />
            <EditText
                android:id="@+id/Nombre1"
                android:hint=""
                android:inputType="text"
                />
            <Button
                android:id="@+id/add1"
                android:background="@drawable/fechaultimate"
                android:layout_height="40dp"
                android:layout_width="10dp"
                />
            <Button
                android:id="@+id/delete1"
                android:background="@drawable/cancel"
                android:layout_height="40dp"
                android:layout_width="5dp"

                />
            <EditText
                android:id="@+id/password1"
                android:clickable="false"
                android:cursorVisible="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                />
        </TableRow>
        <TableRow
            android:background="@android:color/white"
            android:layout_height="match_parent"
            android:layout_width="wrap_content">
            <TextView  android:text="        2"
                android:layout_width="4dp"
                android:layout_height="wrap_content"/>
            <EditText
                android:id="@+id/Phone2"

                android:hint=""
                android:inputType="phone"
                />
            <EditText
                android:id="@+id/Nombre2"

                android:hint=""
                android:inputType="text"
                />
            <Button
                android:id="@+id/add2"
                android:background="@drawable/fechaultimate"
                android:layout_height="40dp"
                android:layout_width="10dp"/>
            <Button
                android:id="@+id/delete2"
                android:background="@drawable/cancel"
                android:layout_height="40dp"
                android:layout_width="5dp"/>
            <EditText
                android:id="@+id/password2"
                android:clickable="false"
                android:cursorVisible="false"
                android:focusable="false"
                android:focusableInTouchMode="false"/>
        </TableRow>
        <TableRow
            android:background="@android:color/white"
            android:layout_height="match_parent"
            android:layout_width="wrap_content">
            <TextView  android:text="        3"
                android:layout_width="4dp"
                android:layout_height="wrap_content"/>
            <EditText
                android:id="@+id/Phone3"

                android:hint=""
                android:inputType="phone"
                />
            <EditText
                android:id="@+id/Nombre3"

                android:hint=""
                android:inputType="text"
                />
            <Button
                android:id="@+id/add3"

                android:background="@drawable/fechaultimate"
                android:layout_height="40dp"
                android:layout_width="10dp"/>
            <Button
                android:id="@+id/delete3"
                android:background="@drawable/cancel"
                android:layout_height="40dp"
                android:layout_width="5dp"
                />
            <EditText
                android:id="@+id/password3"
                android:clickable="false"
                android:cursorVisible="false"
                android:focusable="false"
                android:focusableInTouchMode="false"/>
        </TableRow>
        <TableRow
            android:background="@android:color/white"
            android:layout_height="match_parent"
            android:layout_width="wrap_content">
            <TextView  android:text="        4"
                android:layout_width="4dp"
                android:layout_height="wrap_content"/>
            <EditText
                android:id="@+id/Phone4"
                android:hint=""
                android:inputType="phone"
                />
            <EditText
                android:id="@+id/Nombre4"

                android:hint=""
                android:inputType="text"
                />
            <Button
                android:id="@+id/add4"

                android:background="@drawable/fechaultimate"
                android:layout_height="40dp"
                android:layout_width="10dp"
                />

            <Button
                android:id="@+id/delete4"
                android:background="@drawable/cancel"
                android:layout_height="40dp"
                android:layout_width="5dp"
                />
            <EditText
                android:id="@+id/password4"
                android:clickable="false"
                android:cursorVisible="false"
                android:focusable="false"
                android:focusableInTouchMode="false"
                />
        </TableRow>
    </TableLayout>
</LinearLayout>


</android.support.constraint.ConstraintLayout>
