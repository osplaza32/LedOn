<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.a0ohm.ledpool">

    <!-- To auto-complete the email text field in the login form with the user's emails -->

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ampolletaonsincapaluz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".contoler.LoginActivity" />
        <activity android:name=".contoler.FYPActivity" />
        <activity android:name=".contoler.RegisterActivity" />
        <activity android:name=".contoler.Control_activity" />
        <activity android:name=".contoler.Dashboard" />
        <activity android:name=".contoler.rescue" />
        <activity android:name=".contoler.Question" />
        <activity android:name=".contoler.edit" />
        <activity android:name=".contoler.subUsuarioGestor" />
        <activity android:name=".contoler.loginSub" />
        <activity android:name=".contoler.ControlSub"></activity>
    </application>

</manifest>